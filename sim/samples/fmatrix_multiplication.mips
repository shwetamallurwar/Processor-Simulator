                   ; 5 x 5 double matrix multiplication.
                   MOVE R0, F0
                   LI   R4, 200      ;  elelments are stored row wise.
                   LI   R14, 200 
                   LI   R1, 40       ; R1 = number of elements in row * size of double   
                   LI   R2, 8
                      
       rowcount:   SUB   R4, R4, R1      ; change  the row
                   ADD   R9, R0, R1       ; change the column
                      
       colcount:   SUB   R9, R9, R2       ; decrement B column number
                   ADD.D   F10, F0, F0      ; reset val
                   ADD   R5, R0, R1       ; colm iterator for A
                   ADD   R8, R0, R14      ; row iterator for B
                      
       loop:       SUB   R5, R5, R2       ; decrement column iterator for A
                   SUB   R8, R8, R1      ; decrement row iterator for B
                   ADD    R3, R4, R5      
                   ADD    R7, R8, R9      
                   L.D      F12, 0(R3)      
                   L.D      F13, 0(R7)      
                   MUL.D    F11, F12, F13
                   ADD.D    F10, F10, F11 
                   BNEQ    R5, R0, loop  
                   ADD    R3, R4, R9     
                   S.D     F10, 1600(R3)    ;storing reslts from mem location 1600.
                   BNEQ    R9, R0, colcount   ; loop all colms for one row.
                   BNEQ    R4, R0, rowcount   ; loop all rows
                   EOF R0, R0
                   EOF R0, R0
